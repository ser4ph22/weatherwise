{
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*"]
  },
  "mimeTypes": {
    ".json": "text/json"
  },
  "platform": {
    "apiRuntime": "node:18"
  },
  "skipBuildDuringDeployment": true
}